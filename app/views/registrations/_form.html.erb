<div class="form-horizontal row-fluid">
<div class="span4 well">
<%= form_for(@registration) do |f| %>
  <% if @registration.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@registration.errors.count, "error") %> prohibited this registration from being saved:</h2>

      <ul>
      <% @registration.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-group">
<div class="form-field">
    <%= f.label :user_id %><br>
    <%= f.collection_select :user_id, @users, :id, :first_name, include_blank: true  %>  
  </div>
  </div> 

  <div class="form-group">
  <div class="form-field">
    <%= f.label :product_id %><br>
    <%= f.collection_select :product_id, @products, :id, :model, include_blank: true %>
  </div>

  <div class="form-field">
    <%= f.label :serial %><br>
    <%= f.text_field :serial %>
  </div>
  <div class="form-field">
    <%= f.label :store %><br>
    <%= f.text_field :store %>
  </div>
  <div class="form-field">
    <%= f.label :purchased_at %><br>
    <%= f.date_select :purchased_at %>
  </div>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
</div>
</div>
